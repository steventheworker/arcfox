{
	"manifest_version": 2,
	"name": "ArcFox",
	"version": "1.3.6",
	"description": "Make firefox flow like arc.",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"98": "icon.png"
	},
	"permissions": ["activeTab", "tabs"],
	"background": {
		"scripts": ["src/sidebar.js"]
	},
	"browser_action": {
		"default_icon": {
			"48": "icon.png"
		}
	},
	"sidebar_action": {
		"default_panel": "src/sidebar.html",
		"default_icon": {
			"48": "icon.png"
		}
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "arcfox@steventheworker.com"
		}
	}
}
